//C program to calculate the sum of the digits entered
#include<stdio.h>
void main()
{
	int r,num,sum=0;
	printf("Enter the number\n");
	scanf("%d",&num);
	while(num!=0)
	{
		r=num%10;
		sum=sum+r;
		num=num/10;
	}
	printf("Sum of the digits of the entered number is %d",sum);
}


//C program to list the multiples of a number entered between 1 and 100

#include<stdio.h>
void main()
{
	int i,num;
	printf("Enter the value of num\n");
	scanf("%d",&num);
	printf("The multiples of the number between 1 and 100:\n");
	for(i=num;i<100;i+=num)
	{
	  printf("%d\n",i);
	}
}

//C program to list the even the numbers between M and N
#include<stdio.h>
int main()
{
  int m,n,i;
  printf("Enter the values of m and n\n");
  scanf("%d%d",&m,&n);
  for(i=m;i<=n;i++)
  {
    if(i%2==0)
    {
      printf("%d\n",i);
    }
  }
  return 0;
}		
